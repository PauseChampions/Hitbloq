    <page-button direction='Up' anchor-pos-x='-43' anchor-pos-y='5.5' active='~up-enabled' pref-width='5'
                 on-click='up-clicked'/>
    <page-button direction='Down' anchor-pos-x='-43' anchor-pos-y='-25.5' active='~down-enabled' pref-width='5'
                 on-click='down-clicked'/>
</bg>