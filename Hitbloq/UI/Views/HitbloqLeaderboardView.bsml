<bg>
    <horizontal horizontal-fit='PreferredSize' vertical-fit='PreferredSize' pref-width='100' pref-height='70' pad-left='2'>
        <vertical horizontal-fit='PreferredSize' vertical-fit='PreferredSize' pref-width='7' pref-height='25' pad-bottom='10' pad-top='-10'>
            <vertical-icon-segments data='cell-data' select-cell='cell-selected'/>
        </vertical>
        <vertical horizontal-fit='PreferredSize' vertical-fit='PreferredSize' pref-width='85' pref-height='70' pad-top='3'>
            <leaderboard id='leaderboard' cell-size='4.5'/>
        </vertical>
    </horizontal>

    <button-with-icon icon='Hitbloq.Images.TransparentPixel.png' id='button1' hover-hint='Info' on-click='b-1-click'
                      pref-width='190' pref-height='13' anchor-max-x='.5' anchor-max-y='.5' anchor-min-x='.5'
                      anchor-min-y='.5' anchor-pos-x='3.5' anchor-pos-y='29.8'/>
    <button-with-icon icon='Hitbloq.Images.TransparentPixel.png' id='button2' hover-hint='Info' on-click='b-2-click'
                      pref-width='190' pref-height='13' anchor-max-x='.5' anchor-max-y='.5' anchor-min-x='.5'
                      anchor-min-y='.5' anchor-pos-x='3.5' anchor-pos-y='25.3'/>
    <button-with-icon icon='Hitbloq.Images.TransparentPixel.png' id='button3' hover-hint='Info' on-click='b-3-click'
                      pref-width='190' pref-height='13' anchor-max-x='.5' anchor-max-y='.5' anchor-min-x='.5'
                      anchor-min-y='.5' anchor-pos-x='3.5' anchor-pos-y='20.8'/>
    <button-with-icon icon='Hitbloq.Images.TransparentPixel.png' id='button4' hover-hint='Info' on-click='b-4-click'
                      pref-width='190' pref-height='13' anchor-max-x='.5' anchor-max-y='.5' anchor-min-x='.5'
                      anchor-min-y='.5' anchor-pos-x='3.5' anchor-pos-y='16.3'/>
    <button-with-icon icon='Hitbloq.Images.TransparentPixel.png' id='button5' hover-hint='Info' on-click='b-5-click'
                      pref-width='190' pref-height='13' anchor-max-x='.5' anchor-max-y='.5' anchor-min-x='.5'
                      anchor-min-y='.5' anchor-pos-x='3.5' anchor-pos-y='11.8'/>
    <button-with-icon icon='Hitbloq.Images.TransparentPixel.png' id='button6' hover-hint='Info' on-click='b-6-click'
                      pref-width='190' pref-height='13' anchor-max-x='.5' anchor-max-y='.5' anchor-min-x='.5'
                      anchor-min-y='.5' anchor-pos-x='3.5' anchor-pos-y='7.3'/>
    <button-with-icon icon='Hitbloq.Images.TransparentPixel.png' id='button7' hover-hint='Info' on-click='b-7-click'
                      pref-width='190' pref-height='13' anchor-max-x='.5' anchor-max-y='.5' anchor-min-x='.5'
                      anchor-min-y='.5' anchor-pos-x='3.5' anchor-pos-y='2.8'/>
    <button-with-icon icon='Hitbloq.Images.TransparentPixel.png' id='button8' hover-hint='Info' on-click='b-8-click'
                      pref-width='190' pref-height='13' anchor-max-x='.5' anchor-max-y='.5' anchor-min-x='.5'
                      anchor-min-y='.5' anchor-pos-x='3.5' anchor-pos-y='-1.7'/>
    <button-with-icon icon='Hitbloq.Images.TransparentPixel.png' id='button9' hover-hint='Info' on-click='b-9-click'
